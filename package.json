{
  "name": "common-components-test",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "typings": "dis/index.d.ts",
  "main": "dist/index.js",
  "homepage": "https://my-personal-profile-black.vercel.app/",
  "repository": {
    "url": "https://github.com/Cutshadows/my-storybook",
    "type": "git"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "files": [
    "dist",
    "src"
  ],
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "typescript": ">=4.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "test": "jest -- src/app/ --watch-extensions ts,tsx",
    "test:watch": "jest --watch -- src/app/ --watch-extensions ts,tsx",
    "test:ci": "jest --coverage -- src/app/ --watch-extensions ts,tsx",
    "test:update": "jest --updateSnapshot",
    "format": "prettier --write ./src/app/**/*.{ts,tsx}",
    "check-format": "prettier --check ./src/app/**/*.{ts,tsx}",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.0.0-beta.2",
    "@storybook/addon-interactions": "^8.0.0-beta.2",
    "@storybook/addon-links": "^8.0.0-beta.2",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^8.0.0-beta.2",
    "@storybook/react": "^8.0.0-beta.2",
    "@storybook/react-vite": "^8.0.0-beta.2",
    "@storybook/test": "^8.0.0-beta.2",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.6.15",
    "storybook": "^8.0.0-beta.2",
    "typescript": "^5.2.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2",
    "babel-jest": "^27.4.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^27.5.1",
    "jest-styled-components": "^7.0.8",
    "ts-jest": "^27.1.2",
    "vite": "^5.0.12"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git"
  ]
}
