{
  "name": "components-profile",
  "version": "1.0.0",
  "type": "module",
  "private": false,
  "license": "MIT",
  "main": "dist/components-profile.cjs",
  "homepage": "https://my-personal-profile-black.vercel.app/",
  "repository": {
    "url": "git+https://github.com/Cutshadows/components-profile.git",
    "type": "git"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "typescript": ">=4.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006 --ci --no-open",
    "test": "vitest -- src/app/ --watch-extensions ts,tsx",
    "test:watch": "vitest --watch -- src/app/ --watch-extensions ts,tsx",
    "format": "prettier --write ./src/app/**/*.{ts,tsx}",
    "check-format": "prettier --check ./src/app/**/*.{ts,tsx}",
    "vitest-cover": "vitest run --coverage",
    "chromatic": "chromatic"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-essentials": "^8.1.6",
    "@storybook/addon-interactions": "^8.1.6",
    "@storybook/addon-links": "^8.1.6",
    "@storybook/addon-onboarding": "^8.1.6",
    "@storybook/blocks": "^8.1.6",
    "@storybook/react": "^8.1.6",
    "@storybook/react-vite": "^8.1.6",
    "@storybook/test": "^8.1.6",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^1.6.0",
    "ajv": "^8.16.0",
    "babel-jest": "^27.4.5",
    "chromatic": "^11.3.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.6.15",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^27.5.1",
    "jest-styled-components": "^7.0.8",
    "jsdom": "^24.1.0",
    "prettier": "^2.3.2",
    "storybook": "^8.1.6",
    "ts-jest": "^27.1.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^1.6.0"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git"
  ],
  "preset": "react",
  "module": "dist/common-components-test.js",
  "types": "dist/index.d.ts"
}
